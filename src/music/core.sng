@PRGC_Core

!SONG Core "A|ABBCADC"

=tempo q32
=key 1b  % D minor

!INST 1A PulsePluck   $07 d1/4
!INST 1E PulsePluck   $03 d1/4  % echo
!INST 2A PulsePiano   $06 d1/2
!INST 2B PulsePiano   $04 d1/2  % echo 1
!INST 2C PulsePiano   $03 d1/2  % echo 2
!INST 2D PulsePiano   $02 d1/2  % echo 3
!INST 2E PulsePiano   $01 d1/2  % echo 4
